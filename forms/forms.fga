module forms

extend type tenant
  relations
    define can_create_forms: maintainer or admin

extend type institution
  relations
    define can_create_forms: maintainer or admin

type form
  relations
    define can_add_editor: editor 
    define can_delete: creator
    define can_edit: editor
    define editor: ([user] and member from owner) or maintainer from owner or owner from owner
    define owner: [institution, tenant]
    define creator: [user] and can_create_forms from owner